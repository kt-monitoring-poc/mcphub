{
  "name": "@hades/mcphub",
  "version": "3.0.0",
  "description": "A modern hub server for MCP servers with protocol 2025-06-18 support and complete user management",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcphub": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "mcp_settings.json",
    "servers.json",
    "frontend/dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm backend:build && pnpm frontend:build",
    "backend:build": "tsc",
    "start": "node dist/index.js",
    "backend:dev": "tsx watch src/index.ts",
    "backend:debug": "tsx watch src/index.ts --inspect",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "frontend:dev": "cd frontend && vite",
    "frontend:build": "cd frontend && vite build",
    "frontend:preview": "cd frontend && vite preview",
    "dev": "concurrently \"pnpm backend:dev\" \"pnpm frontend:dev\"",
    "debug": "concurrently \"pnpm backend:debug\" \"pnpm frontend:dev\"",
    "prepublishOnly": "npm run build && node scripts/verify-dist.js",
    "db:validate": "node scripts/validate-schema.js",
    "db:export": "pg_dump postgresql://localhost:5432/mcphub > database-backup.sql",
    "db:import": "psql postgresql://localhost:5432/mcphub < database-backup.sql",
    "typeorm": "typeorm-ts-node-esm",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/db/connection.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/db/connection.ts",
    "api:test": "echo 'API 테스트 스크립트가 삭제되었습니다. TODO-PHASE3-CLEANUP.md를 참조하세요.'",
    "env:validate": "node scripts/env-var-management.js validate",
    "env:cleanup": "node scripts/env-var-management.js cleanup",
    "env:cleanup:dry-run": "node scripts/env-var-management.js cleanup --dry-run",
    "env:report": "node scripts/env-var-management.js report",
    "env:report:detailed": "node scripts/env-var-management.js report --detailed"
  },
  "keywords": [
    "typescript",
    "server",
    "mcp",
    "model context protocol"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^11.0.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.203.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.203.0",
    "@opentelemetry/instrumentation-winston": "^0.48.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@opentelemetry/winston-transport": "^0.14.0",
    "@types/adm-zip": "^0.5.7",
    "@types/multer": "^1.4.13",
    "@types/pg": "^8.15.2",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^12.0.2",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "openai": "^4.103.0",
    "openapi-types": "^12.1.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.16.0",
    "pgvector": "^0.2.1",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
